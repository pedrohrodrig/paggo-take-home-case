
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @default(autoincrement()) @id
  email String @unique
  password String
  invoiceImages InvoiceImage[]
}

model InvoiceImage {
  id Int @default(autoincrement()) @id
  filePath String?
  transcription String?
  transcriptionSummary String?
  userId Int 
  user User @relation(fields: [userId], references: [id])
}
